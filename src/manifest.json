{
  "manifest_version": 2,
  "name": "Better Bandcamp",
  "version": "1.0",
  "description": "Replace bandcamp interface for more keyboard-oriented workflow",
  "content_scripts": [
    {
      "matches": [
        "*://*.bandcamp.com/*"
      ],
      "js": [ "script.ts" ]
    }
  ],
  "web_accessible_resources": [
        "styles/styles.css",
	"pages/*"
  ]
}

